<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GesturePassWordView">
        <attr name="textColor" format="color" />
        <attr name="lineSelect" format="reference" />
        <attr name="orderSelect" format="reference" />
        <attr name="radius" format="integer" />
        <attr name="distance" format="integer" />
        <attr name="bigRadius" format="integer" />

        <attr name="errorCircleColor" format="reference" />
        <attr name="pressedCircleColor" format="reference" />
        <attr name="normalCircleColor" format="reference" />

        <attr name="pressedLineCircleColor" format="reference" />
        <attr name="errorLineCircleColor" format="reference" />

        <attr name="outSideErrorCircleColor" format="reference" />
        <attr name="outSidePressedCircleColor" format="reference" />
        <attr name="outSideNormalCircleColor" format="reference" />

        <attr name="isAlikeInsideCirleErrorColor" format="boolean" />
        <attr name="isAlikeOutsideCirleErrorColor" format="boolean" />

        <attr name="isNotNeedOutSide" format="boolean"/>

        <attr name="outSideStrokeWidth" format="float"/>

        <attr name="maxPwdSize" format="integer"/>
        <attr name="maxCheckTimes" format="integer"/>
        <attr name="lineStrokeWidth" format="integer"/>
        <!--<attr name=""-->
    </declare-styleable>
</resources>